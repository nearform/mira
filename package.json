    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^4.0.6",
    "jest": "^26.4.2",
    "lint-staged": "^11.0.0",
    "mock-fs": "^5.0.0",
    "prettier": "^2.3.0",
    "ts-jest": "^26.3.0",
    "typedoc": "^0.20.36",
    "typescript": "^3.6.5"
  },
