    "yargs": "^15.3.1"
  },
  "peerDependencies": {
    "@aws-cdk/assets": "1.89.0",
    "@aws-cdk/aws-cloudformation": "1.89.0",
    "@aws-cdk/aws-codebuild": "1.89.0",
    "@aws-cdk/aws-codecommit": "1.89.0",
    "@aws-cdk/aws-codepipeline": "1.89.0",
